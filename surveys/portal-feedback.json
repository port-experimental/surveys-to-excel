          }
        },
        "version": {
          "icon": "DefaultProperty",
          "type": "string",
          "title": "Version",
          "default": "Q1 2025",
          "visible": false
        },
        "user": {
          "type": "string",
          "title": "User",
          "blueprint": "_user",
          "format": "entity",
          "default": {
            "jqQuery": ".user.email"
          },
          "visible": false
        },
